# QuizMentor Environment Configuration
# Copy this file to .env and fill in your values

# Environment
EXPO_PUBLIC_ENV=development

# Supabase (your main backend)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key # For admin operations

# Optional: Sentry for Error Tracking (self-hosted available)
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Optional: RevenueCat for in-app purchases
REVENUECAT_API_KEY=your-revenuecat-key
REVENUECAT_APPLE_KEY=your-apple-key
REVENUECAT_GOOGLE_KEY=your-google-key

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_ERROR_TRACKING=true
ENABLE_PERFORMANCE_TRACKING=true
ENABLE_DASHBOARD=true

# Development Settings
DEBUG_MODE=false
LOG_LEVEL=info
