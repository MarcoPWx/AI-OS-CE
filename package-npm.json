{
  "name": "@ai-os/storybook-toolkit",
  "version": "1.0.0-beta.1",
  "description": "AI-OS Developer Toolkit - Production-ready Storybook with AI collaboration tools, GitHub automation, and agent-driven workflows",
  "keywords": [
    "storybook",
    "ai",
    "github",
    "automation",
    "developer-tools",
    "agent",
    "workflow",
    "claude",
    "gpt",
    "copilot",
    "testing",
    "msw",
    "playwright"
  ],
  "homepage": "https://github.com/MarcoPWx/AI-OS-Storybook#readme",
  "bugs": {
    "url": "https://github.com/MarcoPWx/AI-OS-Storybook/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarcoPWx/AI-OS-Storybook.git"
  },
  "license": "MIT",
  "author": "AI-OS Team",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ai-os": "./bin/cli.js",
    "agent-boot": "./bin/agent-boot.js",
    "create-ai-os-storybook": "./bin/create.js"
  },
  "files": [
    "dist",
    "bin",
    "templates",
    ".storybook",
    "src/stories",
    "scripts/gh-workflows.sh",
    ".github/workflows",
    ".github/ISSUE_TEMPLATE",
    "docs",
    "README.md",
    "CONTRIBUTING.md",
    "LICENSE"
  ],
  "scripts": {
    "prepublishOnly": "npm run build && npm run test",
    "postinstall": "node scripts/postinstall.js",
    "build": "tsc && npm run build:storybook",
    "build:storybook": "storybook build",
    "test": "npm run test:unit && npm run test:stories",
    "test:unit": "vitest run",
    "test:stories": "test-storybook",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "msw": "^2.3.1",
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "prompts": "^2.4.2",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.0",
    "@storybook/addon-interactions": "^8.6.0",
    "@storybook/react-vite": "^8.6.0",
    "@storybook/test-runner": "^0.16.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^14.3.1",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@playwright/test": "^1.46.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "http-server": "^14.1.1",
    "jsdom": "^24.0.0",
    "msw-storybook-addon": "^2.0.0",
    "playwright": "^1.46.0",
    "prettier": "^3.3.3",
    "storybook": "^8.6.0",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4",
    "wait-on": "^7.2.0"
  }
}
