{
  "name": "quizmentor",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
"start": "expo start",
    "dev": "npm run start",
    "dev:mock": "npm run start:web:mock",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "eas:build:android:preview": "eas build -p android --profile preview",
    "eas:build:ios:preview": "eas build -p ios --profile preview",
    "eas:build:android:prod": "eas build -p android --profile production-android",
    "eas:build:ios:prod": "eas build -p ios --profile production-ios",
    "eas:submit:android": "eas submit -p android --profile production-android",
    "eas:submit:ios": "eas submit -p ios --profile production-ios",
    "web": "expo start --web",
    "build": "expo build",
    "build:test": "expo build --config=expo.test.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern='(test|spec)\\.(ts|tsx|js|jsx)$' --testPathIgnorePatterns='/e2e/' --coverage",
    "test:unit:json": "mkdir -p data/tests && jest --json --outputFile=data/tests/jest-summary.json",
    "test:integration": "jest --testPathPattern='integration' --runInBand",
"test:e2e": "playwright test",
    "test:smoke:e2e": "playwright test -g @smoke",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:codegen": "playwright codegen",
    "test:e2e:mobile": "cd e2e && ./run-tests.sh mobile all",
    "test:e2e:json": "playwright test --reporter=list,json=playwright-summary.json,html",
    "test:e2e:web:msw": "EXPO_PUBLIC_USE_MSW=1 playwright test e2e/app-msw-journey.spec.ts",
    "e2e:ensure-summary": "node scripts/ensure-playwright-summary.js",
    "test:e2e:detox:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:detox:android": "detox test --configuration android.emu.debug",
    "test:e2e:detox:build:ios": "detox build --configuration ios.sim.debug",
    "test:e2e:detox:build:android": "detox build --configuration android.emu.debug",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run lint && npm run type-check && npm run test:all",
    "tdd": "jest --watch --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.md,.mdx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,md,mdx}'",
    "format:check": "prettier --check '**/*.{ts,tsx,js,jsx,json,md,mdx}'",
    "db:migrate:test": "supabase db push --db-url=$DATABASE_URL",
    "db:seed:test": "node scripts/seed-test-data.js",
    "clean": "rm -rf node_modules coverage playwright-report test-results",
    "reinstall": "npm run clean && npm install",
    "test:performance": "jest --testPathPattern=__tests__/performance/.*\\.(test|spec)\\.[jt]sx?$",
    "test:visual": "playwright test e2e/visual-regression.spec.ts",
    "test:visual:update": "playwright test e2e/visual-regression.spec.ts --update-snapshots",
    "storybook": "storybook dev -p 7007",
    "storybook:ios": "react-native run-ios && npm run storybook",
    "storybook:android": "react-native run-android && npm run storybook",
    "build-storybook": "storybook build",
    "build-storybook:public": "STORYBOOK_OUTPUT_PUBLIC=1 storybook build -o public/storybook",
    "build-storybook:dist": "STORYBOOK_OUTPUT_PUBLIC=1 storybook build -o dist/storybook",
    "build:web": "expo export --platform web",
    "vercel-build": "npm run build:web && npm run build-storybook:dist",
    "test:stories": "test-storybook --ci",
    "serve-storybook": "http-server storybook-static -p 7007 -c-1",
    "serve-storybook:public": "http-server public/storybook -p 7080 -c-1",
    "test:stories:ci": "start-server-and-test 'npm run serve-storybook' http://localhost:7007 'test-storybook --url http://localhost:7007 --ci'",
    "test:sb": "start-server-and-test 'storybook dev -p 7007' http://localhost:7007 'storybook test --url http://localhost:7007 --ci'",
    "test:sb:headed": "start-server-and-test 'storybook dev -p 7007' http://localhost:7007 'storybook test --url http://localhost:7007'",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "e2e:storybook": "start-server-and-test 'storybook dev -p 7007' http://localhost:7007 'playwright test e2e/storybook-msw.spec.ts'",
    "e2e:storybook:json": "start-server-and-test 'storybook dev -p 7007' http://localhost:7007 'playwright test e2e/storybook-msw.spec.ts --reporter=list,json=data/tests/storybook-e2e.json'",
    "e2e:storybook:journeys": "start-server-and-test 'storybook dev -p 7007' http://localhost:7007 'playwright test e2e/storybook-journeys-*.spec.ts'",
    "e2e:storybook:journeys:json": "start-server-and-test 'storybook dev -p 7007' http://localhost:7007 'playwright test e2e/storybook-journeys-*.spec.ts --reporter=list,json=data/tests/storybook-journeys-e2e.json'",
    "e2e:storybook:overlays": "start-server-and-test 'storybook dev -p 7007' http://localhost:7007 'playwright test e2e/storybook-overlays.spec.ts'",
    "test:e2e:sb:overlays": "playwright test -c playwright.storybook.config.ts e2e/storybook-overlays.spec.ts",
    "storybook:with-sse": "concurrently -k -n SSE,SB -c cyan,magenta \"npm run sse:demo\" \"storybook dev -p 7007\"",
    "analyze:storybook": "STATS=1 storybook build",
    "dev:check": "bash scripts/dev-check.sh",
    "start:mock": "USE_MOCKS=true EXPO_PUBLIC_USE_WS_MOCKS=1 expo start",
    "start:web:mock": "EXPO_PUBLIC_USE_MSW=1 expo start --web",
    "start:all-mocks:web": "EXPO_PUBLIC_USE_ALL_MOCKS=1 expo start --web",
    "start:all-mocks:rn": "EXPO_PUBLIC_USE_ALL_MOCKS=1 USE_MOCKS=true expo start",
    "test:mock": "USE_MOCKS=true jest",
    "sse:demo": "node scripts/sse-demo-server.js",
    "export:overview:pdf": "npm run build-storybook && start-server-and-test 'npm run serve-storybook' http://localhost:7007 'node scripts/export-overview-pdf.js'",
    "security:check": "node scripts/security-check.js",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit:fix": "npm audit fix",
    "security:test": "npm run test:stories -- SecurityPlayground",
    "security:all": "npm run security:audit && npm run security:check && npm run security:test",
    "security:ci": "npm run security:all -- --ci",
    "prepare": "husky install",
    "commit": "cz",
    "postbuild": "npm run build-storybook:public"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "@sentry/react-native": "~6.14.0",
    "@supabase/supabase-js": "^2.56.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@types/dompurify": "^3.0.5",
    "axios": "^1.11.0",
    "expo": "~53.0.22",
    "expo-apple-authentication": "~7.2.4",
    "expo-av": "^15.1.7",
    "expo-blur": "^14.1.5",
    "expo-build-properties": "~0.14.8",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.7",
    "expo-device": "~7.1.4",
    "expo-haptics": "^14.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-media-library": "~17.1.7",
    "expo-notifications": "~0.31.4",
    "expo-router": "^5.1.4",
    "expo-splash-screen": "^0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-tracking-transparency": "~5.2.4",
    "firebase": "^12.1.0",
    "isomorphic-dompurify": "^2.26.0",
    "lottie-react-native": "7.2.2",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-mmkv": "^3.3.0",
    "react-native-purchases": "^9.2.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "^0.20.0",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^3.4.17",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@babel/core": "^7.25.2",
    "@gorhom/bottom-sheet": "^5.2.3",
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.55.0",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/slider": "4.5.6",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/test-runner": "^0.17.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-native": "^13.3.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "axios-mock-adapter": "^2.1.0",
    "babel-loader": "^8.4.1",
    "babel-preset-expo": "~13.0.0",
    "chromatic": "^11.12.2",
    "concurrently": "^8.2.2",
    "detox": "^20.20.0",
    "eslint-plugin-mdx": "^3.1.5",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "jest-expo": "~53.0.10",
    "lucide-react": "^0.542.0",
    "msw": "^2.4.1",
    "msw-storybook-addon": "^2.0.3",
    "playwright": "^1.55.0",
    "react-markdown": "^9.0.1",
    "react-native-svg": "15.11.2",
    "react-native-worklets": "^0.4.2",
    "remark-gfm": "^4.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "start-server-and-test": "^2.0.0",
    "storybook": "^8.2.9",
    "supertest": "^7.1.4",
    "swagger-ui-react": "^5.18.2",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3",
    "vite": "^5.4.8",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "eslint-config-prettier": "^9.1.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2"
  },
  "private": true
}
